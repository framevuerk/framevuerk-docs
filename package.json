{
  "name": "framevuerk-docs",
  "description": "Framevuerk Documents",
  "version": "2.4.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest docs --mode production && node ./sitemap-generator.js",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "font-awesome": "^4.7.0",
    "framevuerk": "^2.4.0",
    "idate": "^0.1.5",
    "sitemap": "^2.1.0",
    "vue": "2.5.17",
    "vue-highlightjs": "^1.3.3",
    "vue-meta": "^1.5.5",
    "vue-router": "^2.7.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "node-sass": "4.13.0",
    "prerender-spa-plugin": "^3.4.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "eslintIgnore": [
    "/src/framevuerk/*"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/framevuerk/framevuerk-docs.git"
  },
  "engines": {
    "node": ">= 4.x"
  },
  "author": "nainemom <nainemom@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/framevuerk/framevuerk-docs/issues"
  },
  "homepage": "https://github.com/framevuerk/framevuerk-docs#readme"
}
